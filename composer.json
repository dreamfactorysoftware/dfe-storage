{
    "name":              "dreamfactory/dfe-storage",
    "description":       "DreamFactory(tm) Enterprise Storage Components Package",
    "homepage":          "https://www.dreamfactory.com/",
    "keywords":          [
        "dreamfactory",
        "dfe",
        "fabric",
        "utility",
        "storage"
    ],
    "license":           [
        "proprietary"
    ],
    "authors":           [
        {
            "name":  "Jerry Ablan",
            "email": "jerryablan@dreamfactory.com"
        }
    ],
    "repositories":      [
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-database.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-common.git"
        }
    ],
    "prefer-stable":     false,
    "minimum-stability": "dev",
    "require":           {
        "graham-campbell/flysystem":   "^3.0",
        "league/flysystem-ziparchive": "~1.0",
        "dreamfactory/dfe-common":     "1.1.*"
    },
    "require-dev":       {
        "dreamfactory/dfe-database": "1.1.*",
        "laravel/framework":         "5.2.*",
        "phpunit/phpunit":           "~4.0"
    },
    "autoload":          {
        "psr-4": {
            "DreamFactory\\Enterprise\\Storage\\": "src/"
        }
    },
    "config":            {
        "preferred-install": "dist"
    },
    "extra":             {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
