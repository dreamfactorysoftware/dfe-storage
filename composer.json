{
  "name":              "dreamfactory/dfe-storage",
  "description":       "DreamFactory Enterprise(tm) Storage Library",
  "homepage":          "https://www.dreamfactory.com/",
  "keywords":          [
    "dreamfactory",
    "dfe",
    "fabric",
    "utility",
    "storage"
  ],
  "license":           [
    "proprietary"
  ],
  "authors":           [
    {
      "name":  "Jerry Ablan",
      "email": "jerryablan@dreamfactory.com"
    }
  ],
  "repositories":      [
    {
      "type": "vcs",
      "url":  "https://github.com/dreamfactorysoftware/dfe-database.git"
    },
    {
      "type": "vcs",
      "url":  "https://github.com/dreamfactorysoftware/dfe-common.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable":     true,
  "require":           {
    "dreamfactory/dfe-database": "1.0.*",
    "dreamfactory/dfe-common":   "1.0.*"
  },
  "require-dev":       {
    "phpunit/phpunit":             "@stable",
    "laravel/laravel":             "5.1.*",
    "graham-campbell/flysystem":   "~2.1",
    "league/flysystem-sftp":       "~1.0",
    "league/flysystem-ziparchive": "~1.0"
  },
  "autoload":          {
    "psr-4": {
      "DreamFactory\\Enterprise\\Storage\\": "src/"
    }
  },
  "config":            {
    "preferred-install": "dist"
  },
  "extra":             {
    "branch-alias": {
      "dev-develop": "1.0.x-dev"
    }
  }
}
